generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Cohort {
  AY_2022_23
  AY_2023_24
  AY_2024_25
}

enum Status {
  Online
  Offline
}

enum Courses {
  CBSE_9_Math
  CBSE_9_Science
}

model Students {
  id Int @id @default(autoincrement())
  student_name String
  cohort Cohort
  courses Courses
  date_joined DateTime @default(now())
  last_login DateTime @default(now())
  status Status
}